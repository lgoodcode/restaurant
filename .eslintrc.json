{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "single"],
        "max-len": ["error", 100],
        "object-curly-spacing": ["error", "always"],
        "indent": ["error", 2],
        "space-before-function-paren": ["error", "never"]
    }
}
